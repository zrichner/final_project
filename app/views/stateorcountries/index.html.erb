<div class="page-header">
  <h1>
    Stateorcountries
    <a href="/stateorcountries/new" class="btn btn-lg btn-success">New Stateorcountry</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Region</th>
        <th>Actions</th>
      </tr>

      <% @stateorcountries.each do |stateorcountry| %>
      <tr>
        <td><%= stateorcountry.name %></td>
        <td><%= stateorcountry.region.name %></td>
        <td>
          <a href="/stateorcountries/<%= stateorcountry.id %>" class="btn btn-primary">Show</a>
          <a href="/stateorcountries/<%= stateorcountry.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_stateorcountry/<%= stateorcountry.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
